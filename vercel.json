{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    },
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/components/imports.js",
      "destination": "/components/imports.client.js"
    }
  ],
  "github": {
    "silent": true
  }
}
