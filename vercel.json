{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    },
    {
      "src": "/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "modules/**"
        ]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "github": {
    "silent": true
  }
}
