{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    },
    {
      "src": "/node_modules/es-module-shims/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "/index.*",
      "use": "@vercel/node@canary"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "public": true,
  "github": {
    "silent": true
  }
}
