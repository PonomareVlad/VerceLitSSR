{
  "builds": [
    {
      "src": "/**",
      "use": "@vercel/static"
    },
    {
      "src": "/index.js",
      "use": "@vercel/node@canary"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "public": true,
  "github": {
    "silent": true
  }
}
